#!/bin/sh

DESKTOP=/usr/share/applications/minecraft.desktop
INSTALL=/usr/local/minecraft
JAVA=/usr/java/latest/bin/java
MINECRAFT='http://www.minecraft.net/download/minecraft.jar'

mkdir -p $INSTALL
cd $INSTALL

wget $MINECRAFT

touch $DESKTOP
echo '[Desktop Entry]' >> $DESKTOP
echo 'Name=Minecraft' >> $DESKTOP
echo 'Exec=minecraft' >> $DESKTOP
echo 'Icon=/usr/share/icons/hicolor/48x48/apps/minecraft.png' >> $DESKTOP
echo 'Terminal=false' >> $DESKTOP
echo 'Categories=Game;' >> $DESKTOP
echo 'Type=Application' >> $DESKTOP

echo "$JAVA -Xmx2048M -Xms2048M -cp $INSTALL/minecraft.jar net.minecraft.LauncherFrame" > /usr/bin/minecraft
chmod a+x /usr/bin/minecraft

ICON48=''
ICON32=''
ICON24=''
ICON22=''
ICON16=''

echo $ICON48 | base64 -d > /usr/share/icons/hicolor/48x48/apps/minecraft.png
echo $ICON32 | base64 -d > /usr/share/icons/hicolor/32x32/apps/minecraft.png
echo $ICON24 | base64 -d > /usr/share/icons/hicolor/24x24/apps/minecraft.png
echo $ICON22 | base64 -d > /usr/share/icons/hicolor/22x22/apps/minecraft.png
echo $ICON16 | base64 -d > /usr/share/icons/hicolor/16x16/apps/minecraft.png


